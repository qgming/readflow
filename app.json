{
  "expo": {
    "name": "流畅阅读",
    "slug": "readflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "readflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.qgming.readflow"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./assets/images/icon.png"
  }
}
